{{template "base" .}}
{{define "content"}}
  <div class="container">
    <div class="row">
      <div class="col">
        <h1>This is about page</h1>
        <p>Some text</p>

        <p>this came from template {{index .StringMap "str"}}</p>
        <p>
           {{if ne (index .StringMap "remote_ip" ) ""}}
            remoteIP: {{index .StringMap "remote_ip"}} 
            {{else}}
            No ip address stored
            {{end}}
        </p>
      </div>
    </div>
  </div>
{{end}}